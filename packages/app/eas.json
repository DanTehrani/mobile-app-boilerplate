{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "simulator": {
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://app-api.onrender.com",
        "APP_VARIANT": "development"
      }
    },
    "development": {
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://app-api.onrender.com",
        "APP_VARIANT": "development"
      }
    },
    "test": {
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "distribution": "internal",
      "channel": "test",
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://app-api.onrender.com",
        "APP_VARIANT": "development"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://app-api.onrender.com",
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "xxxx@icloud.com",
        "ascAppId": "xxx",
        "appleTeamId": "xxx"
      }
    }
  }
}
